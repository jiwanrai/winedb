<h1>Add Country</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@country) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_field :description %>

      <%= f.submit "Add Country", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
